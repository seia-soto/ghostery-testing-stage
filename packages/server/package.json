{
  "name": "@testing-stage/server",
  "version": "0.0.1",
  "main": "out/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "start": "NODE_OPTIONS='--import=tsx' node ./src/index.ts",
    "start:watch": "NODE_OPTIONS='--import=tsx' WATCH='true' node ./src/index.ts",
    "test": "NODE_OPTIONS='--import=tsx' ava",
    "bundle": "node scripts/bundle.cjs",
    "standalone": "pkg --no-native-build ."
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "workerThreads": false
  },
  "bin": {
    "ghostery-testing-stage": "./out/bundle/index.cjs"
  },
  "pkg": {
    "assets": "./out/bundle/*.node",
    "targets": ["node18"],
    "outputPath": "./out/binaries"
  },
  "author": "Ghostery GmbH.",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": ">=6.0.0",
    "@typescript-eslint/parser": ">=6.0.0",
    "ava": "^6.0.1",
    "esbuild": "^0.19.10",
    "eslint": ">=8.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "pkg": "^5.8.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@cliqz/adblocker": "^1.26.12",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@sinclair/typebox": "^0.31.28",
    "chokidar": "^3.5.3",
    "fastify": "^4.25.1",
    "fastify-plugin": "^4.5.1"
  }
}
